{
    "name": "memcrab/metrics",
    "description": "Coroutine-compatible metrics client for InfluxDB V2 (Telegraf listener input)",
    "keywords": [
        "metrics",
        "php",
        "influx",
        "influxdb"
    ],
    "homepage": "http://memcrab.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Diudiun",
            "email": "a.diudiun@gmail.com"
        }
    ],
    "type": "package",
    "require": {
        "php": ">=8.0",
        "influxdata/influxdb-client-php": "3.1.0",
        "openswoole/core": "^22.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*.*"
    },
    "autoload": {
        "psr-4": {
            "Memcrab\\Metrics\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "support": {
        "issues": "https://github.com/memcrab/metrics/issues",
        "source": "https://github.com/memcrab/metrics"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
